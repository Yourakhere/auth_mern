{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",  // Ensure the path matches the location of your entry point file
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",  // Routes all requests to your backend index.js
      "dest": "backend/index.js"
    }
  ]
}
